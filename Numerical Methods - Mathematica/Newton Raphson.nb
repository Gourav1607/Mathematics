            RowBox[{"TableHeadings", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{"None", ",", 
               RowBox[{"{", 
                RowBox[{
                "\"\<k\>\"", ",", "\"\<xk\>\"", ",", "\"\<f[xk]\>\""}], 
                "}"}]}], "}"}]}]}], "]"}], ",", "10"}], "]"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{
        "\"\< Root After \>\"", ",", "n", ",", "\"\< Iteration xk = \>\"", 
